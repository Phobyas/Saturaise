  {% comment %} Only check products from relevant collections based on article tags {% endcomment %}
  {% assign collections_to_check = "" | split: "," %}
  {% for tag in article.tags %}
    {% case tag | downcase %}
    {% when 'nike', 'air force', 'dunk' %}
      {% assign collections_to_check = collections_to_check | push: 'nike' %}
    {% when 'yeezy', 'kanye' %}
      {% assign collections_to_check = collections_to_check | push: 'yeezy' %}
    {% when 'jordan', 'air jordan' %}
      {% assign collections_to_check = collections_to_check | push: 'air-jordan' %}
    {% when 'adidas', 'campus', 'samba' %}
      {% assign collections_to_check = collections_to_check | push: 'adidas' %}
    {% when 'new balance', 'nb' %}
      {% assign collections_to_check = collections_to_check | push: 'new-balance' %}
    {% when 'ugg' %}
      {% assign collections_to_check = collections_to_check | push: 'ugg' %}
    {% endcase %}
  {% endfor %}
  
  {% if collections_to_check.size > 0 %}
    {% assign collections_to_check = collections_to_check | uniq %}
    {% for collection_handle in collections_to_check %}
      {% assign collection_products = collections[collection_handle].products %}
      {% for product in collection_products limit: 20 %}
        {% assign product_title_lower = product.title | downcase %}
        {% if article_content_lower contains product_title_lower %}
          {% assign mentioned_products = mentioned_products | push: product %}
          {% if mentioned_products.size >= 5 %}
            {% break %}
          {% endif %}
        {% endif %}
      {% endfor %}
      {% if mentioned_products.size >= 5 %}
        {% break %}
      {% endif %}
    {% endfor %}
  {% endif %}
  
