{% assign min_price = 999999999 %}
{% assign max_price = 0 %}
{% assign available_count = 0 %}
{% for product in collection.products %}
  {% if product.available %}
    {% assign available_count = available_count | plus: 1 %}
    {% if product.price_min < min_price %}
      {% assign min_price = product.price_min %}
    {% endif %}
    {% if product.price_max > max_price %}
      {% assign max_price = product.price_max %}
    {% endif %}
  {% endif %}
{% endfor %}

