<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@graph": [
    {
      "@type": "Organization",
      "@id": "https://saturaise.com/#organization",
      "name": "Saturaise",
      "alternateName": "Saturaise Sneakers",
      "url": "https://saturaise.com",
      "logo": {
        "@type": "ImageObject",
        "url": "https://saturaise.com/cdn/shop/files/logo-new-saturaise2.png",
        "width": 600,
        "height": 60
      },
      "image": "https://saturaise.com/cdn/shop/files/logo-new-saturaise2.png",
      "description": "Saturaise to działająca od trzech lat firma specjalizująca się w sprzedaży najbardziej pożądanych na rynku butów oraz ubrań z gwarancją autentyczności każdego przedmiotu.",
      "address": {
        "@type": "PostalAddress",
        "streetAddress": "Ogrodowa 9a",
        "addressLocality": "Żyrowa",
        "postalCode": "47-330",
        "addressCountry": "PL"
      },
      "contactPoint": {
        "@type": "ContactPoint",
        "telephone": "+48451108247",
        "email": "saturaisekontakt@gmail.com",
        "contactType": "customer service",
        "availableLanguage": "Polish",
        "hoursAvailable": {
          "@type": "OpeningHoursSpecification",
          "dayOfWeek": ["Monday", "Tuesday", "Wednesday", "Thursday", "Friday"],
          "opens": "09:00",
          "closes": "16:00"
        }
      },
      "foundingDate": "2022",
      "areaServed": "PL",
      "sameAs": [
        "https://www.facebook.com/people/Saturaisecom/61576843476406/",
        "https://www.instagram.com/saturaise_com/",
        "https://www.tiktok.com/@saturaise.com"
      ],
      "hasOfferCatalog": {
        "@type": "OfferCatalog",
        "name": "Katalog Saturaise - Limitowane Sneakersy",
        "itemListElement": [
          {
            "@type": "OfferCatalog",
            "name": "Nike",
            "url": "https://saturaise.com/collections/nike"
          },
          {
            "@type": "OfferCatalog", 
            "name": "Adidas",
            "url": "https://saturaise.com/collections/adidas"
          },
          {
            "@type": "OfferCatalog",
            "name": "Yeezy",
            "url": "https://saturaise.com/collections/yeezy"
          },
          {
            "@type": "OfferCatalog",
            "name": "Jordan",
            "url": "https://saturaise.com/collections/air-jordan"
          },
          {
            "@type": "OfferCatalog",
            "name": "New Balance",
            "url": "https://saturaise.com/collections/new-balance"
          },
          {
            "@type": "OfferCatalog",
            "name": "UGG",
            "url": "https://saturaise.com/collections/ugg"
          }
        ]
      },
      "award": "Zaufany Sprzedawca Sneakersów 2024",
      "knowsAbout": ["Sneakersy", "Limitowane edycje", "Streetwear", "Weryfikacja autentyczności", "Nike", "Adidas", "Yeezy", "Jordan", "Travis Scott", "Off-White"],
      "slogan": "Limitowane sneakersy z gwarancją autentyczności",
      "priceRange": "500 PLN - 15000 PLN",
      "paymentAccepted": ["Karta kredytowa", "BLIK", "PayPal", "Apple Pay", "Przelew", "PayPo", "Płatność przy odbiorze"],
      "currenciesAccepted": "PLN",
      "openingHoursSpecification": {
        "@type": "OpeningHoursSpecification",
        "dayOfWeek": ["Monday", "Tuesday", "Wednesday", "Thursday", "Friday"],
        "opens": "09:00",
        "closes": "16:00",
        "validFrom": "2022-01-01",
        "validThrough": "2025-12-31"
      }
    },
    
    {
      "@type": "WebSite",
      "@id": "https://saturaise.com/#website",
      "url": "https://saturaise.com",
      "name": "Saturaise",
      "description": "Limitowane sneakersy Nike, Adidas, Jordan - oryginalne buty i kolekcjonerskie figurki",
      "publisher": {
        "@id": "https://saturaise.com/#organization"
      },
      "inLanguage": "pl-PL",
      "potentialAction": [
        {
          "@type": "SearchAction",
          "target": {
            "@type": "EntryPoint",
            "urlTemplate": "https://saturaise.com/search?q={search_term_string}",
            "actionPlatform": [
              "https://schema.org/DesktopWebPlatform",
              "https://schema.org/MobileWebPlatform"
            ]
          },
          "query-input": {
            "@type": "PropertyValueSpecification",
            "valueRequired": "https://schema.org/True",
            "valueName": "search_term_string"
          }
        }
      ]
    }
    
    {% if template contains 'product' %}
    ,{
      "@type": "WebPage",
      "@id": "{{ shop.url }}{{ product.url }}#webpage",
      "url": "{{ shop.url }}{{ product.url }}",
      "name": "{{ product.title }} | Saturaise",
      "isPartOf": {
        "@id": "https://saturaise.com/#website"
      },
      "breadcrumb": {
        "@id": "{{ shop.url }}{{ product.url }}#breadcrumblist"
      },
      "primaryImageOfPage": {
        "@id": "{{ shop.url }}{{ product.url }}#primaryimage"
      },
      "datePublished": "{{ product.created_at | date: '%Y-%m-%dT%H:%M:%S%z' }}",
      "dateModified": "{{ product.updated_at | default: product.created_at | date: '%Y-%m-%dT%H:%M:%S%z' }}",
      "speakable": {
        "@type": "SpeakableSpecification",
        "cssSelector": [".product__title", ".product__price", ".product__description"]
      }
    }
    ,{
      "@type": "BreadcrumbList",
      "@id": "{{ shop.url }}{{ product.url }}#breadcrumblist",
      "itemListElement": [
        {
          "@type": "ListItem",
          "position": 1,
          "name": "Saturaise",
          "item": "{{ shop.url }}"
        }
        {% if collection.url %}
        ,{
          "@type": "ListItem",
          "position": 2,
          "name": {{ collection.title | json }},
          "item": "{{ shop.url }}{{ collection.url }}"
        }
        ,{
          "@type": "ListItem",
          "position": 3,
          "name": {{ product.title | json }},
          "item": "{{ shop.url }}{{ product.url }}"
        }
        {% else %}
        ,{
          "@type": "ListItem",
          "position": 2,
          "name": {{ product.title | json }},
          "item": "{{ shop.url }}{{ product.url }}"
        }
        {% endif %}
      ]
    }
    {% if product.images.first %}
    ,{
      "@type": "ImageObject",
      "@id": "{{ shop.url }}{{ product.url }}#primaryimage",
      "url": "https:{{ product.images.first | image_url: width: 1200 }}",
      "contentUrl": "https:{{ product.images.first | image_url: width: 1200 }}",
      "width": 1200,
      "height": 1200
    }
    {% endif %}
    ,{
      "@type": "Product",
      "@id": "{{ shop.url }}{{ product.url }}#product",
      "url": "{{ shop.url }}{{ product.url }}",
      "name": {{ product.title | strip_html | json }},
      "description": {% if product.description != blank %}{{ product.description | strip_html | truncatewords: 155 | json }}{% else %}{{ product.title | json }}{% endif %},
      {% if product.type == "Sneakersy" or product.type == "Buty" %}
      "additionalType": "https://schema.org/FootWear",
      {% endif %}
      {% if product.images.size > 0 %}
      "image": [
        {% for image in product.images limit: 10 %}
        "https:{{ image | image_url: width: 1200 }}"{% unless forloop.last %},{% endunless %}
        {% endfor %}
      ],
      {% endif %}
      {% assign product_brand = product.metafields.custom.brand.value | default: product.vendor %}
      {% if product_brand != blank %}
      "brand": {
        "@type": "Brand",
        "name": {{ product_brand | json }}
      },
      "manufacturer": {
        "@type": "Organization", 
        "name": {{ product_brand | json }}
      },
      {% endif %}
      "category": {% if product.type != blank %}{{ product.type | json }}{% else %}"Sneakersy"{% endif %},
      "productID": {{ product.id | json }},
      "sku": {{ product.selected_or_first_available_variant.sku | default: product.selected_or_first_available_variant.id | json }},
      {% if product.selected_or_first_available_variant.barcode != blank %}
        {% assign barcode_length = product.selected_or_first_available_variant.barcode | size %}
        {% if barcode_length == 12 %}
      "gtin12": {{ product.selected_or_first_available_variant.barcode | json }},
        {% elsif barcode_length == 13 %}
      "gtin13": {{ product.selected_or_first_available_variant.barcode | json }},
        {% elsif barcode_length == 14 %}
      "gtin14": {{ product.selected_or_first_available_variant.barcode | json }},
        {% else %}
      "gtin": {{ product.selected_or_first_available_variant.barcode | json }},
        {% endif %}
      {% endif %}
      {% if product.selected_or_first_available_variant.sku or product.selected_or_first_available_variant.barcode or product.metafields.custom.style_code %}
      "identifier": [
        {% if product.selected_or_first_available_variant.sku %}
        {
          "@type": "PropertyValue",
          "propertyID": "SKU",
          "value": {{ product.selected_or_first_available_variant.sku | json }}
        }{% endif %}
        {% if product.selected_or_first_available_variant.barcode %}{% if product.selected_or_first_available_variant.sku %},{% endif %}
        {
          "@type": "PropertyValue", 
          "propertyID": "GTIN",
          "value": {{ product.selected_or_first_available_variant.barcode | json }}
        }{% endif %}
        {% if product.metafields.custom.style_code %}{% if product.selected_or_first_available_variant.sku or product.selected_or_first_available_variant.barcode %},{% endif %}
        {
          "@type": "PropertyValue",
          "propertyID": "StyleCode",
          "value": {{ product.metafields.custom.style_code.value | json }}
        }{% endif %}
      ],
      {% endif %}
      {% if product.metafields.mm-google-shopping.mpn %}
      "mpn": {{ product.metafields.mm-google-shopping.mpn.value | json }},
      {% endif %}
      {% if product.metafields.custom.color %}
      "color": {% if product.metafields.custom.color.value.label %}{{ product.metafields.custom.color.value.label | json }}{% else %}{{ product.metafields.custom.color.value | json }}{% endif %},
      {% elsif product.metafields.shopify.color-pattern %}
      "color": {% if product.metafields.shopify.color-pattern.value.label %}{{ product.metafields.shopify.color-pattern.value.label | json }}{% else %}{{ product.metafields.shopify.color-pattern.value | json }}{% endif %},
      {% endif %}
      {% if product.metafields.shopify.footwear-material %}
      "material": [
        {% for material in product.metafields.shopify.footwear-material.value %}
          {% if material.label %}
          {
            "@type": "Material",
            "name": {{ material.label | json }},
            {% case material.label %}
            {% when "Leather" %}
            "description": "Premium skóra naturalna",
            "materialComposition": "100% skóra bydlęca"
            {% when "Suede" %}
            "description": "Wysokiej jakości zamsz",
            "materialComposition": "100% zamsz naturalny"
            {% when "Canvas" %}
            "description": "Wytrzymałe płótno",
            "materialComposition": "100% bawełna"
            {% when "Synthetic" %}
            "description": "Materiały syntetyczne",
            "materialComposition": "Wysokiej jakości syntetyk"
            {% when "Mesh" %}
            "description": "Oddychająca siateczka",
            "materialComposition": "Materiał tekstylny"
            {% when "Rubber" %}
            "description": "Guma wysokiej jakości",
            "materialComposition": "100% guma"
            {% else %}
            "description": {{ material.label | json }}
            {% endcase %}
          }{% unless forloop.last %},{% endunless %}
          {% endif %}
        {% endfor %}
      ],
      {% endif %}
      {% if product.metafields.custom.size_system or product.metafields.shopify.shoe-size %}
      "hasMeasurement": {
        "@type": "QuantitativeValue",
        "name": "System rozmiarów",
        {% if product.metafields.custom.size_system %}
        "value": {{ product.metafields.custom.size_system.value | json }},
        {% else %}
        "value": "EU",
        {% endif %}
        "unitText": "European Shoe Size"
      },
      {% endif %}
      {% if product.metafields.custom.release_date %}
      "releaseDate": "{{ product.metafields.custom.release_date.value | date: '%Y-%m-%d' }}",
      {% endif %}
      {% if product.metafields.mm-google-shopping.size_type %}
      "sizeSystem": {{ product.metafields.mm-google-shopping.size_type.value | json }},
      {% endif %}
      {% if product.metafields.mm-google-shopping.condition %}
        {% assign condition = product.metafields.mm-google-shopping.condition.value %}
        {% if condition == "new" %}
      "itemCondition": "https://schema.org/NewCondition",
        {% elsif condition == "used" %}
      "itemCondition": "https://schema.org/UsedCondition",
        {% elsif condition == "refurbished" %}
      "itemCondition": "https://schema.org/RefurbishedCondition",
        {% else %}
      "itemCondition": "https://schema.org/NewCondition",
        {% endif %}
      {% else %}
      "itemCondition": "https://schema.org/NewCondition",
      {% endif %}
      "audience": {
        "@type": "PeopleAudience",
        {% if product.metafields.mm-google-shopping.gender %}
        "suggestedGender": {% assign gender = product.metafields.mm-google-shopping.gender.value %}{% if gender == "male" %}"https://schema.org/Male"{% elsif gender == "female" %}"https://schema.org/Female"{% else %}"https://schema.org/Unisex"{% endif %},
        {% elsif product.metafields.shopify.target-gender %}
        "suggestedGender": {% assign gender = product.metafields.shopify.target-gender.value %}{% if gender contains "male" or gender contains "męski" %}"https://schema.org/Male"{% elsif gender contains "female" or gender contains "damski" %}"https://schema.org/Female"{% else %}"https://schema.org/Unisex"{% endif %},
        {% endif %}
        {% if product.metafields.mm-google-shopping.age_group %}
        {% assign age_group = product.metafields.mm-google-shopping.age_group.value %}
        {% if age_group == "kids" %}
        "suggestedMinAge": 3,
        "suggestedMaxAge": 12
        {% elsif age_group == "toddler" %}
        "suggestedMinAge": 1,
        "suggestedMaxAge": 4
        {% elsif age_group == "infant" %}
        "suggestedMinAge": 0,
        "suggestedMaxAge": 2
        {% else %}
        "suggestedMinAge": 16
        {% endif %}
        {% elsif product.metafields.shopify.age-group %}
        {% assign age_group = product.metafields.shopify.age-group.value %}
        {% if age_group contains "kids" or age_group contains "dzieci" %}
        "suggestedMinAge": 3,
        "suggestedMaxAge": 12
        {% else %}
        "suggestedMinAge": 16
        {% endif %}
        {% else %}
        "suggestedMinAge": 16
        {% endif %}
      },
      {% if product.variants.size > 1 %}
      "model": {
        "@type": "ProductModel",
        "name": {{ product.title | split: ' - ' | first | json }},
        {% if product.metafields.custom.style_code %}
        "productModelCode": {{ product.metafields.custom.style_code.value | json }},
        {% endif %}
        "isVariantOf": {
          "@type": "ProductGroup",
          "name": {{ product.title | split: ' - ' | first | json }},
          "productGroupID": "group-{{ product.id }}",
          "hasVariant": [
            {% for variant in product.variants %}
            {
              "@type": "Product",
              "sku": {{ variant.sku | default: variant.id | json }},
              "name": "{{ product.title }} - {{ variant.title }}",
              {% if variant.option1 %}
              "size": {{ variant.option1 | json }},
              {% endif %}
              {% if variant.available %}
              "availability": "https://schema.org/InStock"
              {% else %}
              "availability": "https://schema.org/OutOfStock"
              {% endif %}
            }{% unless forloop.last %},{% endunless %}
            {% endfor %}
          ]
        }
      },
      "hasVariant": [
        {% for variant in product.variants %}
        {
          "@type": "Product",
          "@id": "{{ shop.url }}{{ product.url }}?variant={{ variant.id }}",
          "url": "{{ shop.url }}{{ product.url }}?variant={{ variant.id }}",
          "name": "{{ product.title }} - Rozmiar {{ variant.option1 }}",
          "sku": {{ variant.sku | default: variant.id | json }},
          {% if variant.barcode %}
            {% assign var_barcode_length = variant.barcode | size %}
            {% if var_barcode_length == 12 %}
          "gtin12": {{ variant.barcode | json }},
            {% elsif var_barcode_length == 13 %}
          "gtin13": {{ variant.barcode | json }},
            {% elsif var_barcode_length == 8 %}
          "gtin8": {{ variant.barcode | json }},
            {% else %}
          "gtin": {{ variant.barcode | json }},
            {% endif %}
          {% endif %}
          "size": {{ variant.option1 | json }},
          {% if variant.option2 %}
          "color": {{ variant.option2 | json }},
          {% endif %}
          {% if variant.image %}
          "image": "https:{{ variant.image | image_url: width: 1200 }}",
          {% endif %}
          "weight": {
            "@type": "QuantitativeValue",
            "value": {{ variant.weight | default: 1000 }},
            "unitCode": "GRM"
          },
          "offers": {
            "@type": "Offer",
            "@id": "{{ shop.url }}{{ product.url }}?variant={{ variant.id }}#offer",
            "url": "{{ shop.url }}{{ product.url }}?variant={{ variant.id }}",
            "price": {{ variant.price | money_without_currency | remove: ',' | replace: ',', '.' | json }},
            "priceCurrency": "PLN",
            {% if variant.compare_at_price > variant.price %}
            "priceSpecification": {
              "@type": "PriceSpecification",
              "price": {{ variant.price | money_without_currency | remove: ',' | replace: ',', '.' | json }},
              "priceCurrency": "PLN",
              "valueAddedTaxIncluded": true
            },
            {% endif %}
            "availability": {% if variant.available %}"https://schema.org/InStock"{% elsif variant.inventory_policy == 'continue' %}"https://schema.org/BackOrder"{% else %}"https://schema.org/OutOfStock"{% endif %},
            "inventoryLevel": {
              "@type": "QuantitativeValue",
              "value": {% if variant.inventory_quantity > 0 %}{{ variant.inventory_quantity }}{% else %}0{% endif %},
              "unitCode": "C62",
              "unitText": "sztuk"
            },
            "priceValidUntil": "{{ 'now' | date: '%s' | plus: 2592000 | date: '%Y-%m-%d' }}",
            "seller": {
              "@id": "https://saturaise.com/#organization"
            },
            "eligibleRegion": {
              "@type": "Country",
              "name": "Polska",
              "identifier": "PL"
            }
          }
        }{% unless forloop.last %},{% endunless %}
        {% endfor %}
      ],
      {% endif %}
      "additionalProperty": [
        {% assign properties_added = false %}
        {% if product.metafields.custom.release_date %}
        {
          "@type": "PropertyValue",
          "name": "Data premiery",
          "value": "{{ product.metafields.custom.release_date.value | date: '%Y-%m-%d' }}"
        }{% assign properties_added = true %}
        {% endif %}
        {% if product.metafields.shopify.activity %}
        {% if properties_added %},{% endif %}
        {
          "@type": "PropertyValue",
          "name": "Aktywność",
          "value": {% if product.metafields.shopify.activity.value.first.label %}{% assign activities = "" %}{% for activity in product.metafields.shopify.activity.value %}{% if activity.label %}{% assign activities = activities | append: activity.label %}{% unless forloop.last %}{% assign activities = activities | append: ", " %}{% endunless %}{% endif %}{% endfor %}{{ activities | json }}{% else %}{{ product.metafields.shopify.activity.value | json }}{% endif %}
        }{% assign properties_added = true %}
        {% endif %}
        {% if product.metafields.shopify.shoe-features %}
        {% if properties_added %},{% endif %}
        {
          "@type": "PropertyValue",
          "name": "Cechy obuwia",
          "value": {% if product.metafields.shopify.shoe-features.value.first.label %}{% assign features = "" %}{% for feature in product.metafields.shopify.shoe-features.value %}{% if feature.label %}{% assign features = features | append: feature.label %}{% unless forloop.last %}{% assign features = features | append: ", " %}{% endunless %}{% endif %}{% endfor %}{{ features | json }}{% else %}{{ product.metafields.shopify.shoe-features.value | json }}{% endif %}
        }{% assign properties_added = true %}
        {% endif %}
        {% if product.metafields.shopify.sneaker-style %}
        {% if properties_added %},{% endif %}
        {
          "@type": "PropertyValue",
          "name": "Styl sneakersów",
          "value": {% if product.metafields.shopify.sneaker-style.value.first.label %}{% assign styles = "" %}{% for style in product.metafields.shopify.sneaker-style.value %}{% if style.label %}{% assign styles = styles | append: style.label %}{% unless forloop.last %}{% assign styles = styles | append: ", " %}{% endunless %}{% endif %}{% endfor %}{{ styles | json }}{% elsif product.metafields.shopify.sneaker-style.value.label %}{{ product.metafields.shopify.sneaker-style.value.label | json }}{% else %}{{ product.metafields.shopify.sneaker-style.value | json }}{% endif %}
        }{% assign properties_added = true %}
        {% endif %}
        {% if product.metafields.shopify.closure-type %}
        {% if properties_added %},{% endif %}
        {
          "@type": "PropertyValue",
          "name": "Typ zapięcia",
          "value": {% if product.metafields.shopify.closure-type.value.first.label %}{% assign closures = "" %}{% for closure in product.metafields.shopify.closure-type.value %}{% if closure.label %}{% assign closures = closures | append: closure.label %}{% unless forloop.last %}{% assign closures = closures | append: ", " %}{% endunless %}{% endif %}{% endfor %}{{ closures | json }}{% else %}{{ product.metafields.shopify.closure-type.value | json }}{% endif %}
        }{% assign properties_added = true %}
        {% endif %}
        {% if product.metafields.shopify.shoe-fit %}
        {% if properties_added %},{% endif %}
        {
          "@type": "PropertyValue",
          "name": "Dopasowanie",
          "value": {% if product.metafields.shopify.shoe-fit.value.label %}{{ product.metafields.shopify.shoe-fit.value.label | json }}{% else %}{{ product.metafields.shopify.shoe-fit.value | json }}{% endif %}
        }{% assign properties_added = true %}
        {% endif %}
        {% if product.metafields.custom.size_suggestion %}
        {% if properties_added %},{% endif %}
        {
          "@type": "PropertyValue",
          "name": "Sugestia rozmiaru",
          "value": {{ product.metafields.custom.size_suggestion.value | json }}
        }{% assign properties_added = true %}
        {% endif %}
        {% if properties_added %},{% endif %}
        {
          "@type": "PropertyValue", 
          "name": "Status Autentyczności",
          "value": "100% Oryginalne - Zweryfikowane przez Saturaise"
        },
        {
          "@type": "PropertyValue",
          "name": "Stan",
          "value": "Zupełnie nowe w oryginalnym opakowaniu"
        },
        {
          "@type": "PropertyValue",
          "name": "Czas dostawy",
          "value": "1-3 dni robocze"
        },
        {
          "@type": "PropertyValue",
          "name": "Dostawa",
          "value": "Darmowa w całej Polsce"
        },
        {
          "@type": "PropertyValue",
          "name": "Data dodania",
          "value": "{{ product.created_at | date: '%Y-%m-%d' }}"
        }
      ],
      {% if product.variants.size > 1 %}
      "inProductGroupWithID": {{ product.id | json }},
      {% endif %}
      "hasCertification": [
        {
          "@type": "Certification",
          "name": "Certyfikat Autentyczności Saturaise",
          "certificationIdentification": "SAT-{{ product.id }}-{{ 'now' | date: '%Y%m' }}",
          "certificationRating": {
            "@type": "Rating",
            "ratingValue": "100",
            "bestRating": "100",
            "worstRating": "0",
            "ratingExplanation": "100% gwarancja autentyczności - każdy produkt przechodzi szczegółową weryfikację"
          },
          "issuedBy": {
            "@type": "Organization",
